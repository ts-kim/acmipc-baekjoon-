   "source": [
    "def NChooseK_fast(n, k):\n",
    "    numerator = 1\n",
    "    denominator = 1\n",
    "    k = min(n-k, k) \n",
    "    for i in range(1, k+1):\n",
    "        denominator *= i\n",
    "        numerator *= n+1-i\n",
    "    return numerator//denominator\n",
    "\n",
    "n,k=list(map(int,input().split()))\n",
    "print(NChooseK_fast(n,k)%10007)"
   ]
  }
