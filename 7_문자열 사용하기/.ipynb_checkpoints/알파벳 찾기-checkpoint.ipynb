    "b=[-1 for i in range(26)]\n",
    "for i in range(len(a)):\n",
    "    ok=True\n",
    "    for j in range(i):\n",
    "        if a[i]==a[j]:\n",
    "            ok=False\n",
    "    if ok==True:\n",
    "        b[ord(a[i])-97]=i\n",
    "result=''\n",
    "for i in range(len(b)-1):\n",
    "    result+=str(b[i])+' '\n",
    "result+=str(b[len(b)-1])\n",
    "print(result)"
